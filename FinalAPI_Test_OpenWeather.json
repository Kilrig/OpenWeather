				{
					"name": "Get Weather Info",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Status Code Validation\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"// Response Time Validation\r",
									"pm.test(\"Response time is less than 500ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(500);\r",
									"});\r",
									"\r",
									"// Response Structure Validation\r",
									"pm.test(\"Response contains weather data\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.have.property('weather');\r",
									"});\r",
									"\r",
									"// Response Size Validation\r",
									"pm.test(\"Response size is less than 10KB\", function () {\r",
									"    pm.expect(pm.response.responseSize).to.be.below(10240); // 10KB in bytes\r",
									"});\r",
									"\r",
									"// Content Type Validation\r",
									"pm.test(\"Content-Type is application/json\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\", \"application/json; charset=utf-8\");\r",
									"});\r",
									"\r",
									"// Field Value Validation\r",
									"pm.test(\"City name is London\", function () {\r",
									"    var jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.name).to.eql(\"London\");\r",
									"});\r",
									""
